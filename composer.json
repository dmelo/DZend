{
    "name": "dmelo/dzend",
    "description": "A set of classes to wrap around Zend v1. It' goal is to make common patterns easier to implement and to transparently add commonly used functionalities.",
    "require": {
        "php": ">= 5.4.0",
        "zendframework/zendframework1": ">=1.12.0",
        "facebook/php-sdk-v4": ">=4.0.18"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "emagister/zend-form-decorators-bootstrap": "dev-bootstrap3"
    },
    "autoload": {
        "psr-0": {
            "DZend_": "./library"
        }
    },
    "include-path": [
        "library/"
    ]
}
